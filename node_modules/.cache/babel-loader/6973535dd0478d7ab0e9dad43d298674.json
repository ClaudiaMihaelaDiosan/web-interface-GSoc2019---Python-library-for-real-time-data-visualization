 </div>\n              </v-card-title>\n        <!--      <v-card-actions class=\"justify-center\">\n                <v-btn flat color=\"#0000db\" @click=\"getData()\">Go To ></v-btn>\n              </v-card-actions>  -->\n            </v-card>\n          </v-flex>\n  </v-layout>\n\n\n  <v-card\n   max-width=\"1200\"\n   class=\"mx-auto\"\n >\n <v-toolbar\n     color=\"light-blue\"\n     dark>\n\n     <v-toolbar-title>{{title}}</v-toolbar-title>\n\n     <v-spacer></v-spacer>\n\n     <v-btn icon>\n       <v-icon>search</v-icon>\n     </v-btn>\n\n   </v-toolbar>\n   <v-list subheader>\n     <v-subheader inset> Name </v-subheader>\n     <v-list-item\n       v-for=\"item in roleInfo\"\n       :key=\"item.completeName\"\n       @click=\"\"\n     >\n       <v-list-item-avatar>\n         <v-icon\n           :class=\"[item.icon]\"\n           v-text=\"item.icon\"\n         ></v-icon>\n       </v-list-item-avatar>\n\n       <v-list-item-content>\n         <v-list-item-title v-text=\"item.completeName\"></v-list-item-title>\n       </v-list-item-content>\n\n       <v-list-item-action>\n         <v-btn icon>\n           <v-icon color=\"grey lighten-1\">info</v-icon>\n         </v-btn>\n       </v-list-item-action>\n     </v-list-item>\n  </v-list>\n\n</v-card>\n\n<template>\n<v-card\n    max-width=\"600\"\n    class=\"mx-auto\"\n  >\n    <v-toolbar\n      color=\"red \"\n      dark\n    >\n      <v-app-bar-nav-icon></v-app-bar-nav-icon>\n\n      <v-toolbar-title>My files</v-toolbar-title>\n\n      <v-spacer></v-spacer>\n\n      <v-btn icon>\n        <v-icon>search</v-icon>\n      </v-btn>\n\n      <v-btn icon>\n        <v-icon>view_module</v-icon>\n      </v-btn>\n    </v-toolbar>\n\n    <v-list one-line subheader>\n      <v-subheader :inset=\"inset\">Folders</v-subheader>\n\n      <v-list-item\n        v-for=\"item in items\"\n        :key=\"item.title\"\n        @click=\"\"\n      >\n        <v-list-item-avatar>\n          <v-icon\n            :class=\"[item.iconClass]\"\n            v-text=\"item.icon\"\n          ></v-icon>\n        </v-list-item-avatar>\n\n        <v-list-item-content>\n          <v-list-item-title v-text=\"item.title\"></v-list-item-title>\n          <v-list-item-subtitle v-text=\"item.subtitle\"></v-list-item-subtitle>\n        </v-list-item-content>\n\n        <v-list-item-action>\n          <v-btn icon>\n            <v-icon color=\"grey lighten-1\">info</v-icon>\n          </v-btn>\n        </v-list-item-action>\n      </v-list-item>\n\n      <v-divider inset></v-divider>\n\n      <v-subheader inset>Files</v-subheader>\n\n      <v-list-item\n        v-for=\"item in items2\"\n        :key=\"item.title\"\n        @click=\"\"\n      >\n        <v-list-item-avatar>\n          <v-icon\n            :class=\"[item.iconClass]\"\n            v-text=\"item.icon\"\n          ></v-icon>\n        </v-list-item-avatar>\n\n        <v-list-item-content>\n          <v-list-item-title v-text=\"item.title\"></v-list-item-title>\n          <v-list-item-subtitle v-text=\"item.subtitle\"></v-list-item-subtitle>\n        </v-list-item-content>\n\n        <v-list-item-action>\n          <v-btn icon>\n            <v-icon color=\"grey lighten-1\">info</v-icon>\n          </v-btn>\n        </v-list-item-action>\n      </v-list-item>\n    </v-list>\n  </v-card>\n</template>\n\n\n\n\n<!--  <v-simple-table dark>\n     <thead>\n       <tr>\n         <th class=\"text-left\">Name</th>\n         <th class=\"text-left\">Calories</th>\n       </tr>\n     </thead>\n     <tbody>\n       <tr v-for=\"item in roleInfo\">\n           <td >{{ item.completeName }}</td>\n       </tr>\n   </tbody>\n </v-simple-table>  -->\n\n<!--  <tbody>\n    <tr v-for=\"item in roleInfo\">\n        <td >{{ item.completeName }}</td>\n    </tr>\n</tbody> -->\n\n</v-container>\n\n</template>\n\n<script type=\"text/javascript\">\n\nimport axios from 'axios'\nimport { VListItem, VListItemTitle,VListItemConte } from \"vuetify/lib\";\n\n//const API_URL = \"http://localhost:3000/