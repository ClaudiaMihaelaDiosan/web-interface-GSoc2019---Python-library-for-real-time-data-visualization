v-icon>search</v-icon>\n     </v-btn>\n\n   </v-toolbar>\n   <v-list subheader>\n     <v-subheader inset> Name </v-subheader>\n     <v-list-item\n       v-for=\"item in roleInfo\"\n       :key=\"item.completeName\"\n       @click=\"\"\n     >\n       <v-list-item-avatar>\n         <v-icon\n           :class=\"[item.icon]\"\n           v-text=\"item.icon\"\n         ></v-icon>\n       </v-list-item-avatar>\n\n       <v-list-item-content>\n         <v-list-item-title v-text=\"item.completeName\"></v-list-item-title>\n       </v-list-item-content>\n\n       <v-list-item-action>\n         <v-btn icon>\n           <v-icon color=\"grey lighten-1\">info</v-icon>\n         </v-btn>\n       </v-list-item-action>\n     </v-list-item>\n  </v-list>\n\n</v-card>\n\n<template>\n<v-card\n    max-width=\"600\"\n    class=\"mx-auto\"\n  >\n    <v-toolbar\n      color=\"red \"\n      dark\n    >\n      <v-app-bar-nav-icon></v-app-bar-nav-icon>\n\n      <v-toolbar-title>My files</v-toolbar-title>\n\n      <v-spacer></v-spacer>\n\n      <v-btn icon>\n        <v-icon>search</v-icon>\n      </v-btn>\n\n      <v-btn icon>\n        <v-icon>view_module</v-icon>\n      </v-btn>\n    </v-toolbar>\n\n    <v-list one-line subheader>\n      <v-subheader :inset=\"inset\">Folders</v-subheader>\n\n      <v-list-item\n        v-for=\"item in items\"\n        :key=\"item.title\"\n        @click=\"\"\n      >\n        <v-list-item-avatar>\n          <v-icon\n            :class=\"[item.iconClass]\"\n            v-text=\"item.icon\"\n          ></v-icon>\n        </v-list-item-avatar>\n\n        <v-list-item-content>\n          <v-list-item-title v-text=\"item.title\"></v-list-item-title>\n          <v-list-item-subtitle v-text=\"item.subtitle\"></v-list-item-subtitle>\n        </v-list-item-content>\n\n        <v-list-item-action>\n          <v-btn icon>\n            <v-icon color=\"grey lighten-1\">info</v-icon>\n          </v-btn>\n        </v-list-item-action>\n      </v-list-item>\n\n      <v-divider inset></v-divider>\n\n      <v-subheader inset>Files</v-subheader>\n\n      <v-list-item\n        v-for=\"item in items2\"\n        :key=\"item.title\"\n        @click=\"\"\n      >\n        <v-list-item-avatar>\n          <v-icon\n            :class=\"[item.iconClass]\"\n            v-text=\"item.icon\"\n          ></v-icon>\n        </v-list-item-avatar>\n\n        <v-list-item-content>\n          <v-list-item-title v-text=\"item.title\"></v-list-item-title>\n          <v-list-item-subtitle v-text=\"item.subtitle\"></v-list-item-subtitle>\n        </v-list-item-content>\n\n        <v-list-item-action>\n          <v-btn icon>\n            <v-icon color=\"grey lighten-1\">info</v-icon>\n          </v-btn>\n        </v-list-item-action>\n      </v-list-item>\n    </v-list>\n  </v-card>\n</template>\n\n\n\n\n<!--  <v-simple-table dark>\n     <thead>\n       <tr>\n         <th class=\"text-left\">Name</th>\n         <th class=\"text-left\">Calories</th>\n       </tr>\n     </thead>\n     <tbody>\n       <tr v-for=\"item in roleInfo\">\n           <td >{{ item.completeName }}</td>\n       </tr>\n   </tbody>\n </v-simple-table>  -->\n\n<!--  <tbody>\n    <tr v-for=\"item in roleInfo\">\n        <td >{{ item.completeName }}</td>\n    </tr>\n</tbody> -->\n\n</v-container>\n\n</template>\n\n<script type=\"text/javascript\">\n\nimport axios from 'axios'\nimport { VListItem, VListItemTitle,VListItemConte } from \"vuetify/lib\";\n\n//const API_URL = \"http://localhost:3000/\"\n//const ERIC_API = \"http://192.168.86.30:8112/\"\n\n\n\nexport default {\n  components: {\n    'v-list-item': VListItem,\n    'v-list-item-title': VListItemTitle,\n    'v-list-item-content': VListItemContent\n  },\n  name:'details',\n\n  data: function(){\n    return {\n      cityName : '',\n      imageRole:'',\n      role:'',\n      title:'',\n      roleInfo: [],\n     roleName:[],\n     items: [\n       { icon: 'folder', iconClass: 'grey lighten-1 white--text', title: 'Photos', subtitle: 'Jan 9, 2014' },\n       { icon: 'folder', iconClass: 'grey lighten-1 white--text', title: 'Recipes', subtitle: 'Jan 17, 2014' },\n       { icon: 'folder', iconClass: 'grey lighten-1 white--text', title: 'Work', subtitle: 'Jan 28, 2014' },\n     ],\n     items2: [\n        { icon: 'assignment', iconClass: 'blue white--text', title: 'Vacation itinerary', subtitle: 'Jan 20, 2014' },\n        { icon: 'call_to_action', iconClass: 'amber white--text', title: 'Kitchen remodel', subtitle: 'Jan 10, 2014' },\n      ],\n   }\n },\n\nmounted(){\n  this.cityName = this.$route.params.cityName;\n  this.imageRole = this.$route.params.imageRole;\n  this.role = this.$route.params.role;\n  this.title = this.$route.params.title;\n  this.icon = this.$route.params.icon;\n\n  var vm = this\n  var urlApi = process.env.VUE_APP_NODE_API_URL + this.role + '/' + this.cityName\n//  vm.$router.push({ name: 'details' , params : { title:details.name, cityName:this.city, role: details.role, imageRole:details.img, icon:details.icon }})\n\naxios.get(urlApi)\n.then(function (response){\n  vm.roleInfo = response.data\n  //console.log('formData', response.data)\n\n  vm.roleInfo.forEach((item)=>{\n    //formData define\n    var formData = new FormData()\n    formData.append('id',item.id)\n    formData.append('name',item.completeName)\n    formData.append('longitude',item.location[0])\n    formData.append('latitude',item.location[1])\n    formData.append('range',0)\n    formData.append('description', item.lifeHistory)\n    formData.append('icon', this.icon)\n\n  //  vm.roleName = item.completeName\n  //  console.log(item.completeName)\n\n  })\n})\n.catch(function(response){\n  console.log(\"error\", response);\n})\n},\nmethods: {\n},\n\n}\n\n\n</script>\n"]}]}